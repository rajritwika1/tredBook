{
  "extends": [
    "development"
  ],
  "hints": {
    "button-type": "off",
    "no-inline-styles": "off",
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off",
        "button-name": "off"
      }
    ],
    "axe/forms": [
      "default",
      {
        "label": "off",
        "select-name": "off"
      }
    ],
    "axe/parsing": [
      "default",
      {
        "duplicate-id-active": "off",
        "duplicate-id": "off"
      }
    ],
    "axe/keyboard": [
      "default",
      {
        "nested-interactive": "off"
      }
    ],
    "axe/structure": [
      "default",
      {
        "listitem": "off",
        "list": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "backdrop-filter",
          "scrollbar-width",
          "scrollbar-width: none"
        ]
      }
    ],
    "axe/text-alternatives": [
      "default",
      {
        "input-image-alt": "off"
      }
    ],
    "meta-viewport": "off"
  }
}